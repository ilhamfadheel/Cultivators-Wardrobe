			<ThingDef Type="Item" Name="Item_ProtectCharm" Parent="MiBaoBase">
	<ThingName>Protective Sphere</ThingName>
	<TexPath>res\Sprs\object_zhengfatianxia\object_PTzhu01</TexPath>	
	<FabaoTexPath>res\Sprs\object_zhengfatianxia\object_MBzhu01</FabaoTexPath>
	<Desc>Can save your life once.</Desc>
	<ElementKind>Tu</ElementKind>
	<Item Kind="Equipment">
		<BeMade>
			<CostItems>
				<Item name = "Item_SoulPearl" count = "1"/>
				<Item name = "Item_JinEssence" count = "5"/>
				<Item name = "Item_MuEssence" count = "5"/>
				<Item name = "Item_ShuiEssence" count = "5"/>
				<Item name = "Item_HuoEssence" count = "5"/>
				<Item name = "Item_TuEssence" count = "5"/>
			</CostItems>
		</BeMade> 
		<Fabao>
		    <ActiveCondition>
			    <Condition eventid = "KillCount" des = "Kill 1 Creature while carrying the embryo" needcount = "1" />
			</ActiveCondition>
			<SpecialAbility>
			    <Ability Kind="Sync_AddFlag" sParam1="FLAG_ITEM_RESURRECTION" nParam1 = "1" Desc="At the cost of shattering, resist a death and recover all damage." />
			</SpecialAbility>
		</Fabao>
	</Item>
	<Ling>
		<AddionLing>10</AddionLing>
		<AddionRadius>4</AddionRadius>
		<AddionFailing>0.8</AddionFailing>
		<AddionFailRadius>2</AddionFailRadius>
		
		<Attenuation>0.06</Attenuation>
		<Absorption>0.8</Absorption>
		<Accommodate>34</Accommodate>
	</Ling>
	<Element>
		<Kind>Tu</Kind>
	</Element>
	</ThingDef>
	
